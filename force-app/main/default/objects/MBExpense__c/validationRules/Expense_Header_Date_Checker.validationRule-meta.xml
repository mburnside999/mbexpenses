<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Expense_Header_Date_Checker</fullName>
    <active>true</active>
    <errorConditionFormula>Expense_Date__c &gt; TODAY()</errorConditionFormula>
    <errorDisplayField>Expense_Date__c</errorDisplayField>
    <errorMessage>Expense reports cant be future dated.</errorMessage>
</ValidationRule>
